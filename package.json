{
  "name": "exact-key-map",
  "version": "0.2.0",
  "description": "A tiny utility for creating exact key maps with strong TypeScript typings.",
  "license": "MIT",
  "type": "module",
  "homepage": "https://github.com/higayasuo/exact-key-map#readme",
  "bugs": {
    "url": "https://github.com/higayasuo/exact-key-map/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/higayasuo/exact-key-map.git"
  },
  "author": "Yasuo Higa <higayasuo@gmail.com>",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "npm run typecheck && npm run lint && vite build",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "prepublishOnly": "npm test && npm run build"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "typescript",
    "map",
    "exact",
    "utility",
    "library",
    "vite"
  ],
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  }
}
